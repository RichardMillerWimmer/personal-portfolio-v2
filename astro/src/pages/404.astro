---
import BaseLayout from '../layouts/BaseLayout.astro';

const title = 'Page Not Found';
const description = '';
---

<BaseLayout title={title} description={description} wrapper>
    <h1>404</h1>
    <p>Uh Oh! We couldn't find the page you were looking for. Looks like it doesn't exist or something is under
        construction.</p>
    <a href="/">Return Home</a>
</BaseLayout>